{
  "name": "trello-habitica",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lunkle/trello-habitica.git"
  },
  "description": "Trello Power-Up for Habitica.",
  "version": "1.1.1",
  "main": "webpack.config.dev.js",
  "scripts": {
    "test": "jest",
    "clean-dist": "rimraf dist",
    "copy-files": "cpy src/*.html dist && cpy src/changelog dist/changelog",
    "watch": "webpack --watch",
    "build": "webpack --config webpack.config.prod.js",
    "build-dev": "webpack --config webpack.config.dev.js",
    "prestart": "npm run clean-dist && npm run copy-files",
    "start": "webpack --config webpack.config.dev.js && webpack-dev-server --config webpack.config.dev.js",
    "publish": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "deploy": "npm run build && npm run publish && npm run clean-dist",
    "deploy-dev": "npm run build-dev && npm run publish && npm run clean-dist"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "node-nocache": "^1.0.5"
  },
  "engines": {
    "node": "9.x"
  },
  "keywords": [
    "habitica",
    "habitrpg",
    "trello",
    "power-up",
    "integration"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/runtime": "^7.15.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "cpy-cli": "^3.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^3.4.1",
    "fetch-mock": "^7.7.3",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.5",
    "push-dir": "^0.4.1",
    "rimraf": "^2.7.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "bugs": {
    "url": "https://github.com/Lunkle/trello-habitica/issues"
  },
  "homepage": "https://github.com/Lunkle/trello-habitica#readme",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "author": ""
}
